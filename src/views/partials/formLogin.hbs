<form id="formLogin" action="/datos" method="get">
        <div class="form-group">
          <div class="form-group">
            <label>Email</label>
            <input class="form-control w-50 m-auto" id="email"/>
          </div>
          <div class="form-group">
            <label>Password</label>
            <input type="password" class="form-control w-50 m-auto" id="password"/>
          </div>
        </div>
        <button class="btn btn-success mb-3 mt-2">Ingresar</button>
        <p>¿Aún no tienes cuenta? <a href="/registro">Regístrate</a></p>
        <input type="hidden" name="token" id="token" />
</form>